{
  "rewrites": [
    {
      "source": "/public/(.*)",
      "destination": "/public/$1"
    },
    {
      "source": "/assets/(.*)", 
      "destination": "/assets/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/images/$1"
    },
    
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}